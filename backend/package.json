{
  "name": "Shopper-teste",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsup",
    "dev": "nodemon src/server.ts",
    "start": "node dist/server.js",
    "sequelize": "npx sequelize-cli db:create --config ./dist/database/config.js",
    "migrate": "npx sequelize-cli --config ./dist/database/config.js db:migrate --migrations-path ./dist/database/migrations",
    "seed": "npx sequelize-cli --config ./dist/database/config.js db:seed:all --seeders-path ./dist/database/seeders",
    "drop": "npx sequelize-cli --config ./dist/database/config.js db:drop",
    "db:reset": "npm run build && npm run drop && npm run sequelize && npm run migrate",
    "db:start": "npm run build && npm run migrate",
    "db:dev": "npm run build && npm run migrate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.5",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2",
    "tsup": "^8.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.0",
    "@types/sequelize": "^4.28.20",
    "@typescript-eslint/eslint-plugin": "5.3.1",
    "@typescript-eslint/parser": "5.3.1",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "15.0.0",
    "eslint-plugin-import": "2.25.3",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  }
}
